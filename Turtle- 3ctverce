from turtle import *

drawing_area = Screen()
drawing_area.setup(width=750, height=500)

shape('square')
for i in range(4):
    left(90)
    forward(150)

left(125)
shape('square')
for i in range(4):
    left(90)
    forward(150)

left(125)
shape('square')
for i in range(4):
    left(90)
    forward(150)

done()